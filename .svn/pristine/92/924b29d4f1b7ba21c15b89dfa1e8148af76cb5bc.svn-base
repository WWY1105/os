<form class="row" enctype="multipart/form-data" ng-submit="postSend()" name="add_form" id="add_form">
  <div class="col-xs-12">
    <div class="col-sm-10 form-horizontal">
      <div class="form-group">
        <div class="col-sm-6 text-red col-sm-offset-2 ">*为必填项</div>
      </div>
      <div class="form-group">
        <label for="product" class="col-sm-2 control-label text-right">
          <span class="text-red">*</span> 类型
        </label>
        <div class="col-sm-3" id="product">
          <select class="form-control" ng-model="posts.type" ng-options="item.code as item.text for item in view.type"
                  required></select>
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-2 control-label text-right">
          <span class="text-red">*</span> appKey
        </label>
        <div class="col-sm-3">
          <input type="text" class="form-control" ng-model="posts.appKey" required/>
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-2 control-label text-right">
          <span class="text-red">*</span> appSecret
        </label>
        <div class="col-sm-3 control-label">
          <input type="text" minlength="10" class="form-control" ng-model="posts.appSecret" required/>
        </div>
          <label class="col-sm-3">最少10位</label>
      </div>
      <div class="form-group">
        <label class="col-sm-2 control-label text-right">
          <span class="text-red">*</span> 授权链接
        </label>
        <div class="col-sm-3">
          <input type="url" class="form-control" ng-model="posts.authorUrl" required/>
        </div>
      </div>

      <div class="col-sm-10 col-sm-offset-2">
        <div class="row">
          <div class="col-sm-3">
            <a class="btn btn-default btn-block" back-button>返回</a>
          </div>
          <div class="col-sm-3 col-sm-offset-1">
            <button class="btn btn-danger btn-block" ng-disabled="!add_form.$valid">确定</button>
          </div>
        </div>
      </div>
    </div>
  </div>
</form>
