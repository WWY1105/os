.nav-tabs2 > li > a {
    border: 1px solid #ddd;
    color: black;
    cursor: pointer;
}
.nav-tabs2 > li.active > a, .nav-tabs2 > li.active > a:hover {
    color: #5077aa;
    font-weight: bold;
    background: #d9edf7;
    border-bottom-color: #d9edf7;
}
.nav-tabs2>li {
    width: 74px;
