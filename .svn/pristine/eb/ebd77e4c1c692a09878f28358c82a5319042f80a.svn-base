    display: inline-block;
    margin-bottom: -2px;
}


.accounts_model {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
    width: 100%;
    height: 100%;
    z-index: 3;
}
.accounts_model .pop {
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    margin: auto;
    background: #fff;
    overflow-y: scroll;
    overflow-x: hidden;
    max-height: 850px;
    
}
.accounts_model .pop form {
    padding-top: 80px;
}
.popheader {
    position: absolute;
    width: 100%;
    left: 0;
}

.spotters {
    height: 50px;
    width: 100px;
    background: #2a6496;    
    display: inline-block;
    color: #fff;
    line-height: 50px;
    text-align: center;
    cursor:pointer; 
    border: 1px solid #ccc;
}
.spotActive {
    color: #000;
    background: #fff;
}