* {
    padding: 0;
    margin: 0;
    font-family: "Microsoft Yahei", "宋体", Arial, Helvetica, sans-serif;
}

body {
    height: 100%;
    background: rgb(247, 249, 251);
    padding-bottom: 30px
}

dd {
    table-layout: fixed;
    word-break: break-all;
}

li {
    list-style: none;
}

a:hover {
    text-decoration: none
}

.click {
    cursor: pointer
}

.login_body {
    background: #5f93c4;
}

/*首页背景*/
#footer {
    padding: 40px 0;
    color: #afe0ff;
}

.btn-login {
    background: #69adec;
    color: #fff;
    font-size: 18px;
    font-weight: 700;
    letter-spacing: 8px;
    width: 292px;
}

.btn-login:hover {
    color: #fff;
    background: #5776a4;
}

#login {
    border-top: 6px solid #5776a4;
    width: 100%;
    border-bottom: 12px solid #5776a4;
    margin-top: 170px;
    background: #fff;
    padding: 40px 0
}

#login .img {
    margin-top: 40px;
}

#login .split {
    background: url(../img/split.jpg) 260px top no-repeat;
    height: 180px;
}

#login .login {
    width: 110px;
    background: #69adec;
    border: 0;
    height: 35px;
    line-height: 35px;
    color: #fff;
    font-size: 18px;
    font-weight: bold;
    border-radius: 35px;
}

#login form {
    margin-top: 14px;
}

#login .form-error {
    color: #c45f5f;
    height: 30px;
    width: 100%;
    line-height: 16px;
    text-align: left;
    display: block
}

header {
    background: rgb(80, 119, 170);
    padding: 0 0;
    border-bottom: 1px solid #40689c;
    margin-bottom: 15px;
    width: 100%
}

header {
    z-index: 99;
}

header .header-title {
    line-height: 40px;
    height: 40px;
    font-size: 24px;
}

header .logout {
    color: #fff;
    line-height: 40px
}

header .text {
    color: #fff
}

header .text:hover, header .header-title {
    color: #fff
}

header .logout .btn-link {
    line-height: 16px;
}

header .logout .img {
    padding: 2px 0;
    height: 40px
}

header .logout a {
    color: #fff;
    padding: 0 10px;
    background: #325179;
    display: inline-block;
    height: 100%;
    border-right: 1px solid #40689c
}

header .logout a:hover {
    text-decoration: underline;
}

header .header_user_name {
    padding-right: 15px;
    
}

.left_menu_fixed {
    width: 160px;
}

.left_menu .left_menu_fixed .nav .active a {
    background: #fff;
    color: rgb(75, 101, 134);
    border-radius: 5px;
}

.left_menu .left_menu_fixed .nav .active .son_nav a {
    background: none;
    color: #428bca
}

.left_menu .nav .glyphicon {
    padding-right: 10px
}

.left_menu .left_menu_fixed .nav .son_nav a {
    width: 100%;
    height: 36px;
    display: block;
    padding: 3px 0;
    line-height: 30px;
    font-size: 14px;
    padding-left: 30px;
}

.left_menu .left_menu_fixed .nav .small, .left_menu .left_menu_fixed .nav .son_nav .small {
    font-size: 10px;
    top: 1px
}

.right_content {
    background: #fff;
    min-height: 550px;
    padding-bottom: 30px
}

.right_content .breadcrumb {
    background: #fff;
}

.right_content .results {
    padding-top: 30px;
    overflow: hidden;
}

.btn-search {
    width: 16.6666666%;
    padding: 0 15px;
    display: inline-block;
    margin-left: -15px;
    margin-bottom: 0px;
    vertical-align: middle;
}

table td {
    font-size: 12px;
    font-family: "宋体";
    vertical-align: middle !important;
}

table th {
    color: #5077aa;
    text-align: center;
}

table .error {
    color: red
}

.btn-half {
    width: 70px;
    margin-right: 5px
}

.pagination {
    margin-top: 0;
    margin-bottom: 0;
}

.right_content .add_step li {
    width: 178px;
    height: 30px;
    line-height: 30px;
    float: left;
    text-align: center;
    background: url(../img/step2.png) right center no-repeat;
    margin-left: -8px;
    color: #3e4f69
}

.right_content .add_step li:first-child {
    width: 158px;
    height: 30px;
    line-height: 30px;
    float: left;
    text-align: center;
}

.right_content .add_step .action {
    background-image: url(../img/step.png);
    color: #fff
}

.right_content .add_step {
    padding-left: 30px;
    padding-bottom: 60px;
}

.right_content .logo_show {
    width: 112px;
    height: 112px;
    border: 1px solid #000
}

.form-inline .text-right {
    line-height: 34px;
}

.right_content .pd2 {
    padding-right: 20px
}

.clear {
    clear: both;
    overflow: hidden;
}

textarea {
    min-height: 100px;
    resize: none;
}

.check-out {
    text-align: center;
    line-height: 34px;
    color: #666
}

.check-out input {
    display: none;
}

.check-out .check, .check-out .radio {
    border: 1px solid #ccc;
    border-radius: 4px;
    height: 34px;
    padding-top: 0
}

.check-out .active {
    border-color: #428bca;
    color: #428bca;
    background: url(../img/checkbg.png) right bottom no-repeat;
}

.pic_show {
    width: 100%;
    border: 1px solid #ccc;
    color: #ccc;
    border-radius: 4px;
    height: 145px;
    line-height: 125px;
    text-align: center;
    font-size: 26px;;
}

.input-text {
    line-height: 26px;
    padding-top: 20px;
}

.submite {
    padding: 40px 0;
}

.edit_step {
    margin-bottom: 40px;
}

.edit-show-text {
    line-height: 34px;
}

.eye {
    position: absolute;
    line-height: 30px;
    left: 26px;
    z-index: 99;
    background: #fff;
    top: 2px;
    width: 220px
}

.text-left {
    text-align: left !important
}

.error {
    color: red
}

.click_list a {
    padding: 0 5px;
    height: 100%;
    display: inline-block;
    width: 50%
}

.click_list a:hover {
    background: #2a6496;
    color: #fff
}
.modal-dialog {
    max-height: 50rem;
    overflow: auto;
}
.modal-body {
    /* height: 37.5rem 600/16; */
    overflow: auto
}

#dialog_password_edit .eye {
    width: 160px
}

.help-block {
    margin-bottom: 0px
}

.results .pagesize {
    line-height: 30px
}

.important {
    color: red;
    padding: 0 5px;
    font-weight: 700
}

.state .btn-group .btn {
    border: 1px solid #ccc;
}

.state .btn-group .btn:first-child {
    border-right: 0;
}

.state .btn-group .btn:last-child {
    border-left: 0;
}

.state .off.active {
    background: #d9534f;
    color: #fff;
}

.state .on.active {
    background: #31b0d5;
    color: #fff;
}

.licencePicShow img, .legalPicShow img {
    width: 100%
}

.legalPicShow, .licencePicShow {
    border-radius: 4px 4px 0px 0px;
    border: 1px solid #ccc;
    overflow: hidden;
    border-bottom: 0
}

.legalPicChose, .licencePicChose {
    border-radius: 0px 0px 4px 4px
}

.viewShow {
    width: 600px;
}

.viewShow img {
    max-width: 100%;
}

.select-btn .this i {
    display: block;
    float: left;
    top: 2px;
}

.select-btn i {
    display: none;
}

.select-btn .this {
    border-color: #357ebd;
    color: #357ebd
}

.zone-show span, .zone-show .add {
    cursor: pointer;
    margin-bottom: 10px;
    background: rgb(233, 243, 252);
    text-align: center;
    display: block;
    height: 40px;
    width: 100%;
    font-size: 16px;
    line-height: 40px
}

.zone-show .active span, .zone-show a.add:hover, #District-show .active {
    background: rgb(85, 120, 176);
    color: #fff;
}

.zone-show {
    color: #5578b0
}

.zone-show h4 {
    margin-bottom: 15px
}

.zone-show > div {
    position: relative;
}

.zone-show > .col-sm-12 > div > .glyphicon {
    position: absolute;
    top: 0;
    right: 20px;
    top: 13px;
    display: none;
    cursor: pointer;
}

.zone-show > .col-sm-12 > div:hover > .glyphicon {
    display: block;
}

.zone-show > .col-sm-12 > .active > .glyphicon {
    color: #fff;
}

.zone-show .add {
    display: block;
    background: #fff;
}

.displaynone {
    display: none;
}

.zone-tab {
    margin-right: 10px;
}

.zone-tab span {
    padding: 0 10px
}

.ng-hide {
    visibility: hidden;
    display: none;
}

.plan-son .version .popover {
    display: block;
}

/*mine*/
.yellow-btn, .yellow-btn:hover, .yellow-btn:focus {
    background: #fbe144;
}

.yellow-border-btn.yellow-border-btn:hover, .yellow-border-btn:focus {
    border: 1px solid #fbe144;
    background: white
}

.red-border-btn.red-border-btn:hover, .red-border-btn:focus {
    border: 1px solid red;
    background: white
}

#editor {
    max-height: 250px;
    height: 250px;
    background-color: white;
    border-collapse: separate;
    border: 1px solid rgb(204, 204, 204);
    padding: 4px;
    box-sizing: content-box;
    -webkit-box-shadow: rgba(0, 0, 0, 0.0745098) 0px 1px 1px 0px inset;
    box-shadow: rgba(0, 0, 0, 0.0745098) 0px 1px 1px 0px inset;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;
    border-top-left-radius: 3px;
    overflow: scroll;
    outline: none;
}

.border .text-yellow {
    color: #fbe144;
    padding-bottom: 1px;
    border-bottom: 2px solid #fbe144
}

.border {
    border: 1px solid #ddd;
    padding: 15px;
    font-size: 24px
}

#dialog_comment .nav-tabs > li.active {
    border-bottom: 2px solid #fbe144;
}

#dialog_comment .nav-tabs {
    float: right;
}

#dialog_comment .nav-tabs > li > a, #dialog_comment .nav-tabs > li.active > a:hover, #dialog_comment .nav-tabs > li.active > a:focus {
    color: #555;
    cursor: default;
    background-color: #fff;
    border-left: 1px solid #ddd;
}

#dialog_comment .nav-tabs {
    border-bottom-width: 0;
}

.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus {
    border-width: 0;
}

.nav-tabs > li > a {
    margin-right: 0;
}

.nav-tabs > li > button {
    border-left: 1px solid #ddd;
}

.close {
    line-height: 2;
    color: #555;
    text-shadow: 0 1px 0 #fff;
    opacity: 1;
    padding: 10px;
}

.form-bg {
    background: #f0f0f0;
    padding: 10px;
}

.form-bg .form-control {
    font-size: 12px;
    padding: 3px 6px;
    line-height: 26px;
}

.text-xs {
    font-size: 12px;
}

.title {
    font-size: 18px;
    margin: 10px 0 7px 10px;
    white-space: nowrap;
    width: 40%;
    overflow: hidden;
    text-overflow: ellipsis;
}

.row.box {
    margin: 20px 15px 0;
    padding-bottom: 15px;
    border-bottom: 1px solid #ddd;

}

.text-yellow {
    color: #de9930;
}

.mb10 {
    margin-bottom: 10px;
}

.text-lg {
    font-size: 16px;
}

.page {
    padding: 30px 15px 45px;
}

.btn-layout {
    padding: 2px 20px;
    margin: 10px 0 0;
}

.img-xs {
    width: 2em;
    height: 2em;
}

img {
    max-width: 100%;
}

.selected_mask {
    width: 30%;
    display: inline-block;
    margin: 5px;
    position: relative;
}

.selected_mask_icon {
    position: absolute;
    top: 0;
    left: 0;
    background: rgba(0,0,0,.3) url(https://res.wx.qq.com/mpres/htmledition/images/icon/common/icon_card_selected3a7b38.png) no-repeat 0 0;
    width: 100%;
    height: 100%;
    vertical-align: middle;
    display: inline-block;
    background-position: 50% 50%;
}

.time-search {
    /*border-bottom: 1px solid #eeeeee;*/
    padding: 13px 20px 6px 12px;
    position: relative;
}

.time-search.project label {
    width: 100%;
}
.form-inline .form-control {
    line-height: inherit;
}
.btn {
    font-size: 12px;
}

.couponadd .form-inline {
    margin: 10px 0;
    clear: both;
}


.form-inline .form-control {
    line-height: inherit;
}

.couponadd .col-sm-2 {
    margin-top: 3px;
    line-height: inherit;

}
.text-red {
    color: #ee6363;
    padding-right: 5px;
}
.modal-header {
    border-bottom-color: #0297CF;
}
.modal-header {
    border-radius: 5px 5px 0 0;
    background: rgb(250, 250, 250);
}
.ng-invalid, .ng-valid-pattern.ng-invalid-required {
    border-color: #0297CF;
}
option {
