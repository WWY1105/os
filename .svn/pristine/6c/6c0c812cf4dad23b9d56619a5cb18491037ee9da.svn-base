<div class="contents bgw">
  <table class="table table-bordered table-striped text-center">
    <thead>
    <tr class="info">
      <th>序号</th>
      <th>类型</th>
      <th>appKey</th>
      <th>appSecret</th>
      <th>授权链接</th>
      <th>启用</th>
      <th>操作</th>
    </tr>
    </thead>
    <tbody>
    <tr ng-repeat="cashiers in view.cashiers.items">
      <td ng-bind="$index+1"></td>
      <td ng-bind="cashiers.name"></td>
      <td ng-bind="cashiers.appKey"></td>
      <td ng-bind="cashiers.appSecret"></td>
      <td ng-bind="cashiers.authorUrl"></td>
      <td ng-bind="cashiers.enabled?'是':'否'"></td>
      <td>
        <a class="click" ng-if="!cashiers.enabled" ng-click="switch(cashiers.id,'on')">启用</a>
        <a class="click" ng-if="cashiers.enabled" ng-click="switch(cashiers.id,'off')">停用</a>
        <a class="click" ng-click="delete($index)"><i class="glyphicon glyphicon-remove"></i>删除</a>
      </td>

    </tr>
    </tbody>
  </table>
  <div class="col-sm-8 " ng-if="view.cashiers.items">
    <pagination items-per-page="view.cashiers.count" boundary-links="true" total-items="view.cashiers.total"
                ng-model="view.cashiers.page" class="pagination-sm" previous-text="&lsaquo;" ng-change="pageChange()"
                next-text="&rsaquo;" first-text="&laquo;" last-text="&raquo;">
    </pagination>

  </div>

  <div class="col-sm-3">
    <a href="/cashiers/index.html#/cashiers/add" class="btn btn-primary">新增</a>
  </div>
</div>
