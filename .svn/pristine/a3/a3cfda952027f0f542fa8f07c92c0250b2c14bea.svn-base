	<body class="login_body">
		<div id="login">
			<div class="container">
				<div class="row">
					<div class="col-md-3"></div>
					<div class="col-md-3 split">
						<img class="img" src="/public/img/login_logo.jpg"/>
					</div>
					<div class="col-md-3 ">
						    <form class="form-horizontal"   id="login_form" >
						    	  <label class="control-label form-error" ></label>
							    <div class="form-group ">
									 <div class="input-group">
							          <div class="input-group-addon"><i class="glyphicon glyphicon-user"></i></div>
							          <input class="form-control" type="text" name="name" placeholder="输入用户名">
							        </div>
								</div>
							     <div class="form-group">
									 <div class="input-group">
							          <div class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></div>
							          <input class="form-control" name="password" type="password" placeholder="输入密码">
							        </div>
								</div>
								<div class="form-group">
									 <button class="btn btn-login" >登录</button>
								</div>
						    </form>
					</div>

				</div>
			</div>
		</div>
